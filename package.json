{
  "name": "verticals-frontend",
  "version": "1.0.1",
  "description": "Frontend part for verticals project",
  "scripts": {
    "start": "webpack serve --config ./build/webpack.dev.config.js",
    "i18n:extract": "i18next-scanner",
    "analyze": "node ./build/analyze.js",
    "build": "webpack --config ./build/webpack.build.config.js",
    "ts:check": "npx tsc --pretty",
    "lint": "npm run ts:check && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "npm run ts:check && eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "sb": "start-storybook -s ./src/public -p 6006",
    "sb:build": "build-storybook -s ./src/public",
    "loki:prepend": "node ./.loki/replace.js",
    "loki:update": "npm run sb:build && npm run loki:prepend && npx loki --chromeDockerUseCopy --verboseRenderer --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test": "jest --config ./jest.config.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "jest --config ./jest.config.js --coverage",
    "test:stryker": "npx stryker run",
    "ci": "npm run lint:fix && npm run test && npm run loki:update",
    "graph": "npx madge --image graph.svg"
  },
  "author": "Max Romanyuta <xom9ik.code@gmail.com>",
  "license": "MIT",
  "keywords": [
    "verticals",
    "checklist",
    "tasks",
    "todo",
    "organize",
    "work",
    "list",
    "workspace"
  ],
